#header {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    height: 30px;
    background: #005800;
}

#header i {
    position: absolute;
    top: 5px;
    left: 8px;
    color: #fff;
}

#header h2 {
    font-size: 18px;
    height: 30px;
    line-height: 30px;
    text-align: center;
    color: #fff;
}

#wrap {
    position: absolute;
    top: 30px;
    left: 0;
    right: 0;
    bottom: 0;
}

.head {
    position: absolute;
    top: 13px;
    left: 0;
    right: 0;
    height: 60px;
}

.head h3 {
    text-align: center;
    height: 30px;
    line-height: 30px;
    margin-bottom: 5px;
}

.body {
    position: absolute;
    top: 71px;
    bottom: 0;
    left: 0;
    right: 0;
    background-color: #eee;
    overflow: auto;
}
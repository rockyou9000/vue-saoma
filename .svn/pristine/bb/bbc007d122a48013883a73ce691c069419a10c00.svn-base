/*list css*/

.head {
    font-size: 14px;
}

.head label {
    color: #666;
    display: block;
    font-size: 12px;
    margin: 10px auto;
    width: 300px;
}

.head .date_sel {
    height: 25px;
    width: 100px;
    border: 1px solid #ccc;
    text-indent: 5px;
}

.head .paper_num {
    border: 1px solid #ccc;
    height: 25px;
    width: 120px;
    text-indent: 5px;
}

.head button {
    width: 50px;
    height: 25px;
    background-color: #32bbd1;
    color: #fff;
    margin-left: 15px;
    border-radius: 5px;
}

.body .list_item {
    font-size: 12px;
    padding-top: 8px;
    padding-bottom: 8px;
    color: #333;
    border-bottom: dashed 1px #cccccc;
}

.body .list_item p {
    padding: 2px 10px;
}

.body .list_item .list_l {
    display: inline-block;
    width: 50%;
    vertical-align: top;
}

#pan_list_wrap .body {
    background-color: #eee;
    bottom: 0;
    left: 0;
    overflow: auto;
    position: absolute;
    right: 0;
    top: 92px;
}


/*detail css*/

.detail_head {
    font-size: 14px;
    height: 140px;
    left: 0;
    padding: 8px;
    position: absolute;
    right: 0;
    background-color: #ffffff;
}

.detail_body {
    font-size: 14px;
    position: absolute;
    right: 0;
    top: 170px;
    left: 0;
    height: 88px;
    background-color: #ffffff;
}

.detail_head p {
    color: #333333;
    font-size: 14px;
    margin: 0;
    padding-bottom: 3px;
}

.detail_head h3 {
    font-size: 14px;
    margin-bottom: 3px;
}

.detail_body h3 {
    background: #ffffff none repeat scroll 0 0;
    font-size: 14px;
    padding-left: 8px;
    padding-top: 8px;
}

.detail_head .list_l {
    width: 45%;
}

.detail_head .filter_input input {
    border: 1px solid #ccc;
    color: #333;
    margin: 0 6px;
    padding: 5px;
    width: 180px;
}

.detail_head .filter_input button {
    background: #22cb64 none repeat scroll 0 0;
    border-radius: 3px;
    color: #fff;
    height: 26px;
    width: 50px;
}

.detail_head .sao_type_sys {
    background: #60a80f;
}

.detail_body .edit_btn {
    padding-left: 8px;
    padding-top: 10px;
    background-color: #fff;
}

.detail_body .edit_btn li {
    padding-left: 10px;
    padding-right: 10px;
    padding-top: 5px;
    padding-bottom: 5px;
    float: left;
    text-align: center;
    background-color: red;
    border: solid 1px #22cb64;
    background-color: #fff;
    margin-right: 5px;
    border-radius: 5px;
    color: #22cb64;
}

.detail_list {
    position: absolute;
    right: 0;
    top: 248px;
    bottom: 0;
    left: 0;
    background-color: #eee;
    overflow: auto;
}

.detail_list thead {
    width: calc( 100% - 1em);
}

.detail_list tbody {
    display: block;
    height: 195px;
    overflow-y: auto;
}

.detail_list thead,
.detail_list tbody tr {
    display: table;
    width: 100%;
    table-layout: fixed;
}

.wrapbg_for_list {
    background-color: #e7e7e7;
}
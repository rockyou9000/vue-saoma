#header {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    height: 40px;
    background: #22cb64;
}

#header i {
    color: #fff;
    font-size: 14px;
    font-style: normal;
    left: 8px;
    position: absolute;
    top: 10px;
}

#header h2 {
    color: #fff;
    font-size: 16px;
    font-weight: normal;
    height: 40px;
    line-height: 38px;
    text-align: center;
}

#wrap {
    position: absolute;
    top: 40px;
    left: 0;
    right: 0;
    bottom: 0;
}

.head {
    position: absolute;
    left: 0;
    right: 0;
    height: 130px;
}

.head h3 {
    background-image: url("http://pic.lqxshop.com/sources/pandian/ico_pandian.png");
    background-position: 8px 10px;
    background-repeat: no-repeat;
    color: #333333;
    font-size: 18px;
    font-weight: normal;
    padding-bottom: 24px;
    padding-left: 56px;
    padding-top: 22px;
    text-align: left;
}

.body {
    background-color: #eee;
    bottom: 0;
    left: 0;
    overflow: auto;
    position: absolute;
    right: 0;
    top: 130px;
}
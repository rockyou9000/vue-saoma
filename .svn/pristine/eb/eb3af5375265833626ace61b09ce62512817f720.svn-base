.head {
    font-size: 14px;
}

.head label {
    width: 300px;
    display: block;
    margin: 0 auto 5px;
}

.head .date_sel {
    width: 100px;
    border: 1px solid #ccc;
}

.head .paper_num {
    width: 120px;
    border: 1px solid #ccc;
    margin: 0 5px;
}

.head button {
    width: 50px;
    height: 25px;
    background-color: #60a80f;
    color: #fff;
    border: 1px solid #ccc;
    margin-left: 15px;
}

.body .list_item {
    font-size: 13px;
    color: #333;
    background-color: #f1ab46;
    margin: 5px;
}

.body .list_item p {
    padding: 2px 10px;
}

.body .list_item .list_l {
    display: inline-block;
    width: 60%;
}